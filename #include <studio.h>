#include<studio.h>
Int main()
{
Int marks;
Printf("Enter a number ");
Scanf"%d",&mark);
If ( Mark>85 && mark<=100)
{
Printf("grade A");
}
Else if (mark>70 && mark<=84)
{
Printf("grade B");
}
Else if (mark>55 && mark<=69)
{
Printf("grade C");
}
Else if (mark>40 && mark<=54)
{
Printf("grade D");
}
Else if (mark<40)
{
Printf("grade F");
}
}
